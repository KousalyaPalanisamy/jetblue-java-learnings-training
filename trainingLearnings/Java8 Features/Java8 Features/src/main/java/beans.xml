<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:component-scan
		base-package="com.learnJava.runPrograms"></context:component-scan>

	<bean id="OperationsOn8"
		class="java8.operations.MapVsFlatMap"></bean>

	<bean id="sort" class="collection.operations.SortWithComps">
		<property name="employeeID" value="1646" />
		<property name="employeeName" value="Greer" />
		<property name="employeeDesignation"
			value="Senior Software Developer" />
	</bean>


</beans>
